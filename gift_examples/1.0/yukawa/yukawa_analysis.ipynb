   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 6. Extract Mass Ratios"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"\\n\" + \"=\"*60)\n",
    "print(\"MASS RATIO EXTRACTION\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "# Method 1: From diagonal elements of original tensor\n",
    "print(\"\\nMethod 1: Diagonal Elements Y_{iii}\")\n",
    "print(\"-\" * 40)\n",
    "diagonal = np.array([np.abs(Y[i, i, min(i, 76)]) for i in range(min(21, 77))])\n",
    "top_3_indices = np.argsort(diagonal)[-3:][::-1]  # Top 3 in descending order\n",
    "top_3_values = diagonal[top_3_indices]\n",
    "\n",
    "print(f\"Top 3 diagonal elements:\")\n",
    "for i, (idx, val) in enumerate(zip(top_3_indices, top_3_values)):\n",
    "    print(f\"  Gen {i+1}: |Y_{{{idx},{idx},{idx}}}| = {val:.6e}\")\n",
    "\n",
    "if top_3_values[2] > 1e-10:  # Avoid division by near-zero\n",
    "    ratio_32 = top_3_values[0] / top_3_values[1]  # Gen3/Gen2 (top/charm)\n",
    "    ratio_21 = top_3_values[1] / top_3_values[2]  # Gen2/Gen1 (charm/up)\n",
    "    print(f\"\\nMass ratios (from diagonal):\")\n",
    "    print(f\"  Gen3/Gen2 (â‰ˆ m_t/m_c): {ratio_32:.2f}\")\n",
    "    print(f\"  Gen2/Gen1 (â‰ˆ m_c/m_u): {ratio_21:.2f}\")\n",
    "else:\n",
    "    ratio_32 = 0.0\n",
    "    ratio_21 = 0.0\n",
    "    print(\"\\nâš  Diagonal elements too small for reliable ratios\")\n",
    "\n",
    "# Method 2: From Tucker core (if available)\n",
    "if TENSORLY_AVAILABLE and core is not None:\n",
    "    print(\"\\nMethod 2: Tucker Core Diagonal\")\n",
    "    print(\"-\" * 40)\n",
    "    core_diag = np.array([np.abs(core[i, i, i]) for i in range(3)])\n",
    "    print(f\"Core diagonal elements:\")\n",
    "    for i, val in enumerate(core_diag):\n",
    "        print(f\"  Gen {i+1}: |core_{{{i},{i},{i}}}| = {val:.6e}\")\n",
    "    \n",
    "    if core_diag[1] > 1e-10 and core_diag[2] > 1e-10:\n",
    "        ratio_32_tucker = core_diag[2] / core_diag[1]\n",
    "        ratio_21_tucker = core_diag[1] / core_diag[0]\n",
    "        print(f\"\\nMass ratios (from Tucker core):\")\n",
    "        print(f\"  Gen3/Gen2 (â‰ˆ m_t/m_c): {ratio_32_tucker:.2f}\")\n",
    "        print(f\"  Gen2/Gen1 (â‰ˆ m_c/m_u): {ratio_21_tucker:.2f}\")\n",
    "    else:\n",
    "        ratio_32_tucker = 0.0\n",
    "        ratio_21_tucker = 0.0\n",
    "else:\n",
    "    ratio_32_tucker = None\n",
    "    ratio_21_tucker = None\n",
    "\n",
    "# GIFT theoretical predictions\n",
    "gift_ratios = {\n",
    "    'top_charm': 57.5,\n",
    "    'charm_up': 20.0,\n",
    "    'tau_muon': 16.8\n",
    "}\n",
    "\n",
    "print(\"\\n\" + \"=\"*60)\n",
    "print(\"COMPARISON WITH GIFT PREDICTIONS\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "print(f\"\\nm_t/m_c (Top/Charm):\")\n",
    "print(f\"  GIFT prediction:     {gift_ratios['top_charm']:.1f}\")\n",
    "print(f\"  Neural (diagonal):   {ratio_32:.1f}\")\n",
    "if ratio_32_tucker is not None:\n",
    "    print(f\"  Neural (Tucker):     {ratio_32_tucker:.1f}\")\n",
    "if ratio_32 > 0:\n",
    "    deviation_diag = abs(ratio_32 - gift_ratios['top_charm']) / gift_ratios['top_charm']\n",
    "    print(f\"  Deviation (diagonal): {deviation_diag*100:.1f}%\")\n",
    "if ratio_32_tucker is not None and ratio_32_tucker > 0:\n",
    "    deviation_tucker = abs(ratio_32_tucker - gift_ratios['top_charm']) / gift_ratios['top_charm']\n",
    "    print(f\"  Deviation (Tucker):   {deviation_tucker*100:.1f}%\")\n",
    "\n",
    "print(f\"\\nm_c/m_u (Charm/Up):\")\n",
    "print(f\"  GIFT prediction:     {gift_ratios['charm_up']:.1f}\")\n",
    "print(f\"  Neural (diagonal):   {ratio_21:.1f}\")\n",
    "if ratio_21_tucker is not None:\n",
    "    print(f\"  Neural (Tucker):     {ratio_21_tucker:.1f}\")\n",
    "if ratio_21 > 0:\n",
    "    deviation_diag = abs(ratio_21 - gift_ratios['charm_up']) / gift_ratios['charm_up']\n",
    "    print(f\"  Deviation (diagonal): {deviation_diag*100:.1f}%\")\n",
    "if ratio_21_tucker is not None and ratio_21_tucker > 0:\n",
    "    deviation_tucker = abs(ratio_21_tucker - gift_ratios['charm_up']) / gift_ratios['charm_up']\n",
    "    print(f\"  Deviation (Tucker):   {deviation_tucker*100:.1f}%\")\n",
    "\n",
    "# Store mass ratio results\n",
    "mass_ratios = {\n",
    "    'method_diagonal': {\n",
    "        'top_charm': float(ratio_32),\n",
    "        'charm_up': float(ratio_21),\n",
    "        'top_3_indices': top_3_indices.tolist(),\n",
    "        'top_3_values': top_3_values.tolist()\n",
    "    },\n",
    "    'gift_predictions': gift_ratios\n",
    "}\n",
    "\n",
    "if ratio_32_tucker is not None:\n",
    "    mass_ratios['method_tucker'] = {\n",
    "        'top_charm': float(ratio_32_tucker),\n",
    "        'charm_up': float(ratio_21_tucker)\n",
    "    }"
   ]
